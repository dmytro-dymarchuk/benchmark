{
  "name": "spryker-sdk/benchmark",
  "type": "library",
  "description": "Benchmark module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "phpbench/phpbench": "^0.17.0",
    "spryker/application": "^3.0.0",
    "spryker/glue-application": "^1.27.0",
    "spryker/guzzle": "^2.1.0",
    "spryker/kernel": "^3.53.0",
    "spryker/symfony": "^3.1.0",
    "spryker/util-encoding": "^2.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/config": "*",
    "spryker/error-handler": "*"
  },
  "autoload": {
    "psr-4": {
      "SprykerSdk\\": "src/SprykerSdk/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
